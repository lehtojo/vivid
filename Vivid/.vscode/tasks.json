{
	"version": "2.0.0",
	"tasks": [
		{
			"label": "Build",
			"command": "dotnet",
			"type": "process",
			"args": [
				"build",
				"${workspaceFolder}/Vivid.csproj"
			],
			"problemMatcher": "$msCompile",
			"presentation": {
				"reveal": "silent"
			},
			"group": {
				"kind": "build",
				"isDefault": true
			}
		},
		{
			"label": "Units",
			"command": "dotnet",
			"type": "process",
			"args": [
				"build",
				"${workspaceFolder}/Vivid.csproj",
				"-c",
				"Units"
			],
			"problemMatcher": "$msCompile",
			"presentation": {
				"reveal": "silent"
			},
			"group": {
				"kind": "build",
				"isDefault": true
			}
		}
	]
}